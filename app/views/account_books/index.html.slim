- set_meta_tags title: t('.title')
- breadcrumb :account_books
.px-1.text-gray-500
  .flex.flex-row.mx-auto.justify-center.mb-5.mt-3
    = image_pack_tag 'logo-no-border.png', size: '200x200'
  .text-right.mb-5
    = render 'search_form', url: account_books_path, q: @q
  .grid.grid-cols-1.mb-5.md:grid-cols-2.lg:mx-5.xl:grid-cols-3.gap-4
    = render @account_books
  .flex.justify-center
    = paginate(@account_books)
